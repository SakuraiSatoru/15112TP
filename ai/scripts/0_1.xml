<?xml version="1.0" ?>
<bulletml type="vertical"
          xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">
    <action label="top">
        <fire>
            <bullet>
                <direction type="absolute">180</direction>
                <speed>2</speed>
                <action>
                    <wait>60</wait>
                    <changeSpeed>
                        <speed>0</speed>
                        <term>30</term>
                    </changeSpeed>
                </action>
            </bullet>
        </fire>
    </action>
</bulletml>